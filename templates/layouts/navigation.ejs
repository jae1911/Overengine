    <small id="dotsies">Dotsies?</small>

    <% if(locals.domains && domains.length > 0) { %>
        <hr>
        <p>Served on:</p>
        <ul>
            <% domains.forEach((domain) => { %>
                <% if(domain.includes('.onion')) { %>
                    <li><a href="http://<%- domain %>"><%- domain %></a></li>
                <% } else { %>
                    <li><a href="//<%- domain %>"><%- domain %></a></li>
                <% } %>
            <% }) %>
        </ul>
    <% } %>

