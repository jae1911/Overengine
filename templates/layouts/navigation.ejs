    <div class="therflag">
        <img src="/files/img/tdelta.svg" alt="Delta Theta symbol of Theriantropy" class="timg">
    </div>

    <script type="text/javascript">
        var element = document.getElementById("body");
        function changeStyle() {
            if (element.style.fontFamily != "dotsies")
            {
                element.style.fontFamily = "dotsies";
                localStorage.setItem("curfont", "dotsies");
            }
            else
            {
                element.style.fontFamily = "pixel";
                localStorage.setItem("curfont", "pixel");
            }
        }
        element.style.fontFamily = localStorage.getItem("curfont"); 
        document.getElementById("dotsies").addEventListener("click", changeStyle);
    </script>
</div>
